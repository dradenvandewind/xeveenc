git clone https://github.com/mpeg5/xeve && \
cd xeve && \
mkdir build && \
cd build && \
cmake .. && \
make && \
make install && \
ldconfig


git clone https://github.com/dradenvandewind/xeveenc.git && \
cd xeveenc && \
meson builddir --prefix=/usr && \
ninja -C builddir && \
ninja -C builddir install && \
ldconfig



